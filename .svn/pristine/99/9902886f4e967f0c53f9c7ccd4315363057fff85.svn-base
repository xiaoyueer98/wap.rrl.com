.rec_hd{
    width: 100%;
    height: 44px;
    background: #206daf;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
}
.rec_hd .l{
    width: 12px;
    height: 21px;
    background: url(../images/l_jt.png) no-repeat;
    background-size: cover;
    display: block;
    position: absolute;
    left: 10px;
    top: 11px;
}


 .rec_hd .r{
    width: 22px;
    height: 22px;
    background: url(../images/hd_r.png) no-repeat;
    background-size: 22px 22px;
    display: block;
    position: absolute;
    right: 10px;
    top: 12px;
