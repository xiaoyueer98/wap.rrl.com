} 
.rec_hd>ul{
    width: 31.25%;
    margin: 0 auto;
    height: 18px;
    color: #ffffff;
    padding-top:15px;
    background: url(../images/recm_hd_c.png) no-repeat right 20px;
    background-size:18.6px 9.3px ;
    padding-right:18px;
}
.rec_hd>.hd_li>li{
    width: 100%;
    color: #ffffff;
    height: 18px;
    text-align: center;
}

.rec_hd>.hd_li>ul{
    display: none;
    width: 100%;
    margin: 0 auto;
    color: #ffffff;
    background: #ffffff;
    border:1px solid #cfcfcf;
    padding:10px 0;
    margin-top:-4px;
}
.rec_hd>.hd_li>b{
    width: 12px;
    height: 7px;
    background: url(../images/hd_ul_t.png) no-repeat;
    background-size: cover;
    display: none;
    position: relative;
    top: -3px;
    left: 10px;
    z-index: 2;
}
.rec_hd>.hd_li>ul>a>li{
    height: 22px;
    line-height: 22px;
    width: 100%;
    text-align: center;
    color: #333333;
    font-size: 12px;
}
.rec_hd>.hd_li>ul>a>li.m{
    background: #dfdfdf;
}
.rec_hd .r2{
    width: 33px;
    height: 22px;
    display: block;
    position: absolute;
    right: 10px;
    top: 15px;
    color: #ffffff;
    -webkit-transform:rotate(0deg);
} 

