body,button,input,select,textarea{  font-family:'微软雅黑', Geneva, sans-serif arial;font-size: 12px;}
/* reset */
html{ color:#343434; -webkit-text-size-adjust:100%; -ms-text-size-adjust:100%;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section,iframe{ margin:0; padding:0;}
/*body,button,input,select,textarea{ font:12px arial,"Microsoft YaHei"}*/
input,select,textarea{ font-size:12px; font-family:'微软雅黑'}
table{ border-collapse:collapse; border-spacing:0;}
th{ text-align:inherit;}
fieldset,img{ border:0;}
img{margin:0;padding:0;border:0;}
iframe{ display:block;}
input::-webkit-input-placeholder{color:#fff; font-weight:bold;}
input::-moz-input-placeholder{color:#fff; font-weight:bold;}
abbr,acronym{ border:0; font-variant:normal;}
del{ text-decoration:line-through;}
address,caption,cite,code,dfn,em,i,th,var{ font-style:normal; font-size: 12px;}
ol,ul{ list-style:none; font-family:'微软雅黑'}
caption,th { text-align:left; font-family:'微软雅黑'}
h1,h2,h3,h4,h5,h6{ font-size:12px; font-family:'微软雅黑'}
q:before,q:after{ content:'';}  
ins,a{ text-decoration:none; font-family:'微软雅黑'}
button{ border:0; outline:none; font-family:'微软雅黑'}
.wk{white-space:normal;word-wrap:break-word;word-break:break-all;}
input::-webkit-input-placeholder { color: #D6D0CA !important;  WebKit browsers } input:-moz-placeholder { color: #D6D0CA !important; } input::-moz-placeholder { color: #D6D0CA !important}
body,html{
    width: 100%;
    height: 100%;
}
.clearfix:after{ visibility:hidden; display:block; font-size:0; content:" "; clear:both; height:0;}
.clearfix{ zoom:1;}
