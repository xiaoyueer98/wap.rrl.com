<ul class="head-nav" style='display:none;'>
    <volist name="lArr" id="vo"  key="k" empty="">
        <li class="">
            <a href="{$vo.url}">
                <span class="dis-block sp-img">
                    <img src="{$vo.img}" alt="">
                </span>
                <span class="dis-block sp-text">{$vo.name}</span>
            </a>
        </li>
    </volist>

</ul>
<script>
    $(function () {
        $(".head-nav li").each(function () {

            if (location.href.indexOf($(this).find("a").attr("href")) != "-1" && $(this).find("a").attr("href") != "/") {
                this.className = "m";
            } else {
                
                this.className = "";
            }
        })
    })
</script>