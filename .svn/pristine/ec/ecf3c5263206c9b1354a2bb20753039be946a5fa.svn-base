html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
	font-family: '微软雅黑'
	}

.rec_hd{
	width: 100%;
	height: 44px;
	background: #206daf;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 2;
}
.rec_hd .l{
	width: 12px;
	height: 21px;
	background: url(/Public/images/l_jt.png) no-repeat;
	background-size: cover;
	display: block;
	position: absolute;
	left: 10px;
	top: 11px;
}


.rec_hd .r{
	width: 22px;
	height: 19px;
	background: url(/Public/images/hd_r.png) no-repeat;
	background-size: cover;
	display: block;
	position: absolute;
	right: 10px;
	top: 12px;
}
.rec_hd>ul{
	width: 31.25%;
	margin: 0 auto;
	height: 18px;
	color: #ffffff;
	padding-top:15px;
	background: url(/Public/images/recm_hd_c.png) no-repeat right 20px;
	background-size:18.6px 9.3px ;
	padding-right:0px;
}
.rec_hd>.hd_li>li{
	width: 100%;
	color: #ffffff;
	height: 18px;
	text-align: center;
}

.rec_hd>.hd_li>ul{
	display: none;
	width: 100%;
	margin: 0 auto;
	color: #ffffff;
	background: #ffffff;
	border:1px solid #cfcfcf;
	padding:10px 0;
	margin-top:-4px;
}
.rec_hd>.hd_li>b{
	width: 12px;
	height: 7px;
	background: url(/Public/images/hd_ul_t.png) no-repeat;
	background-size: cover;
	display: none;
	position: relative;
	top: -3px;
	left: 10px;
	z-index: 2;
}
.rec_hd>.hd_li>ul>a>li{
	height: 22px;
	line-height: 22px;
	width: 100%;
	text-align: center;
	color: #333333;
	font-size: 12px;
}
.rec_hd>.hd_li>ul>a>li.m{
	background: #dfdfdf;
}

ul li{list-style:none;}

.wapper li.first_li{
        margin-top:50px;
	width: 100%;
	border-bottom:1px #9f9f9f solid;
}
.wapper textarea{
	width: 100%;
	height: 74px;
	display: block;
	border: none;
	background: #ffffff;
	resize:none;
	overflow: hidden;
	outline: medium;
	font-size: 14px;
	line-height: 20px
}
.wapper li.myli_2{
	padding:10px 0;
	border-bottom:1px #9f9f9f solid;
}
.wapper li.myli_2 span{
	display: block;
	color: #2380b8;
	font-size: 14px;
	line-height: 20px
}
.wapper li.myli_2 p{
	display: block;
	color: #2380b8;
	font-size: 14px;
	line-height: 20px
}
.wapper li.last_li{
	height: 58px;
	padding: 15px 20px;
}

.wapper li.last_li button.btn_qr{
	width: 113px;
	height: 27px;
	background: #2380b8;
	color: #ffffff;
	font-size: 14px;
	border: none;
	outline: medium;
	float: left;
	margin-right:30px;
	border-radius: 5px;
}
.wapper li.last_li button.btn_qx{
	width: 111px;
	height: 25px;
	background: #ffffff;
	color: #2380b8;
	font-size: 14px;
	border: none;
	outline: medium;
	border-radius: 5px;
	float: right;
	border:2px #2380b8 solid;
}

.mask{
	position: fixed;
	z-index: 3;
	display: block;
	width: 100%;
	height: 100%;
	background: #000;
	opacity: .7;
	filter: alpha(opacity=70);
}
.tjcg{
	width: 300px;
	height: 200px;
	padding:10px 0;
	background: #ffffff;
	box-shadow: 1px 1px 10px #2c2b2b;
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left:-150px;
	margin-top: -90px;
	z-index: 4;
	border-radius: 15px;
}
.tjcg dl{
	margin:0 auto;
	width: 260px;
}
.tjcg dl dt{
	width:80px;
	margin:0 auto;
}
.tjcg dl dt img{
	width: 100%;
}

.tjcg dl dd{
	text-align: center;
	color: #2380b8;
	line-height: 30px;
	font-size: 16px;
	font-weight: bold;
}
.tjcg dl dd button{
	width: 110px;
	height: 30px;
	background: #2380b8;
	border-radius: 5px;
	border:none;
	color: #ffffff;
	margin-top:10px;
}

