html,body{
	max-width: 100%;
	height: 100%;
}
*{
	font-family: '微软雅黑'；
}
.clearfix:after {
    clear: both;
    display: block;
    visibility: hidden;
    height: 0;
    content: " ";
    font-size: 0;
}
.cur{
	cursor:pointer;
}
.dis_box{
	display: -webkit-box !important;
	display: -moz-box !important;
	display: -mz-box !important;
	display: -o-box !important;
}
.aligncenter{
	-webkit-box-align:center;
	-moz-box-align:center;
	-mz-box-align:center;
	-o-box-align:center;
}
.alignright{
	-webkit-box-align:right;
	-moz-box-align:right;
	-mz-box-align:right;
	-o-box-align:right;
}
.packcenter{
	-webkit-box-pack:center;
	-moz-box-pack:center;
	-mz-box-pack:center;
	-o-box-pack:center;
}
.packright{
	-webkit-box-pack:right;
	-moz-box-pack:right;
	-mz-box-pack:right;
	-o-box-pack:right;
}
.orient{
	-webkit-box-orient: vertical;
	-moz-box-orient: vertical;
	-mz-box-orient: vertical;
	-o-box-orient: vertical;
	
}
.flex{
	-webkit-box-flex:1;
	-moz-box-flex: 1;
	-mz-box-flex:1;
	-o-box-flex:1;
}
#container{
	display: -webkit-box;
	display: -moz-box;
	display: -mz-box;
	display: -o-box;
	-webkit-box-orient: vertical;
	-moz-box-orient: vertical;
	-mz-box-orient: vertical;
	-o-box-orient: vertical;
	height: 100%;
	width: 100%;
	margin:0 auto;
}
button{
	outline: medium;
}
input{
	 outline: none;
	 outline: medium;
}
ul,li{list-style: none;}

a{
	text-decoration:none;
}






/* ------------------------weppage-------------------------- */
.wapper{
	max-width:640px;
	height: 100%;
	margin: 0 auto;
}
.headder{
  width: 100%;
  height: 43px;
  background: #3d638a;
  border-bottom: 2px #1b5d95 solid;
  text-align: right;
  line-height: 43px;
  font-size: 14px;
  position: absolute;
  z-index: 2;
  top: 1;
}
.headder>.nav_btn{
	width: 25px;
	position: absolute;
	top: 5px;
	left: 10px;
	display: block;
}
.headder>.logo{
	width: 87px;
