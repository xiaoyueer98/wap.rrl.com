function sun()
{
        $i   = 5;
        $sum = 0;
        for ($j = 0; $j <= 10; $j++)
        {
                if ($j == $i)
                {
                        break;
                }
                else
                {
                        $sum +=$j;
                        echo $sum."<br/>";
                }
        }
        echo $sum;
}
