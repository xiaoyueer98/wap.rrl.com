//设置session
function sessions()
{

        ini_set("session.cookie_lifetime", 60);
        ini_set("session.gc_maxlifetime", 60);
        ini_set("session.save_path", "/var/www/wap_renrenlie/tmp/");
        ini_set("session.gc_divisor", 1);

        session_start();
        $_SESSION['test'] = "test";
}

